<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Remove-Item-from-Menu-Response">
    <DisplayName>Remove Item from Menu Response</DisplayName>
    <Set>
        <Payload contentType="text/plain">Removed itemID = {menuRequest.itemID} from menuID = {menuRequest.menuID}</Payload>
        <StatusCode>200</StatusCode>
        <ReasonPhrase>Success</ReasonPhrase>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>