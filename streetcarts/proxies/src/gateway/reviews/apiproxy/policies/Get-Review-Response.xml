<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Get-Review-Response">
    <DisplayName>Get Review Response</DisplayName>
    <Set>
        <Payload contentType="text/plain">Get specifc review: reviewID = {reviewRequest.reviewID}</Payload>
        <StatusCode>200</StatusCode>
        <ReasonPhrase>Success</ReasonPhrase>
