<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Add-Review-Response">
    <DisplayName>Add Review Response</DisplayName>
    <Set>
        <Payload contentType="text/plain">Added user review: userID = {reviewRequest.userID}</Payload>
        <StatusCode>201</StatusCode>
        <ReasonPhrase>Success</ReasonPhrase>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>