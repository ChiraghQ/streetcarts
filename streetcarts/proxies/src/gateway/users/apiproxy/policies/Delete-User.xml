<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Delete-User">
    <DisplayName>Delete User Response</DisplayName>
    <Set>
        <Payload contentType="text/plain">Deleted specific user: userID = {userRequest.userID}</Payload>
        <StatusCode>200</StatusCode>
        <ReasonPhrase>Success</ReasonPhrase>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>