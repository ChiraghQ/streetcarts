<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Get-User-Response">
    <DisplayName>Get User Response</DisplayName>
    <Set>
        <Payload contentType="text/plain">Got specific user: userID = {userRequest.userID}</Payload>
        <StatusCode>200</StatusCode>
        <ReasonPhrase>Success</ReasonPhrase>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>